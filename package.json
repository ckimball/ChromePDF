{
  "name": "chrome-pdf",
  "version": "1.0.0",
  "description": "Package to build Windows executable to export HTML from URL to PDF",
  "license": "MIT",
  "main": "chrome-pdf.js",
  "dependencies": {
    "pkg": "^4.3.4",
    "puppeteer": "^1.10.0",
    "rimraf": "^2.6.2",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "cryptiles": ">=4.1.2"
  },
  "scripts": {
    "build": "pkg chrome-pdf.js --targets node8-win-x64",
    "clean": "rimraf chrome-pdf.exe && rimraf *.log && rimraf *.pdf"
  }
}
